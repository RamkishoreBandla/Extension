{
    "manifest_version":2,
    "name":"Job filler",
    "version":"1.0",
    "description":"To autofill the job application forms",
    "permissions":["activeTab","https://*/*","http://*/*"],
    "content_scripts":[
        {
            "matches":["https://*/*","http://*/*"],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"icon.png"
    }
}